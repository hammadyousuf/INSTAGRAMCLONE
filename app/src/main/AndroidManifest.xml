<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kashdevelopment.development.kashmirstudentportal">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="com.kashdevelopment.development.kashmirstudentportal.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=" کشمیر طالب علم"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label=" کشمیر طالب علم"
            android:theme="@style/AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.PostDetailsActivity"
            android:configChanges="orientation|screenSize"
            android:label="Post details(پوسٹ کی تفصیلات)" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.CreatePostActivity"
            android:configChanges="orientation|screenSize"
            android:label="Create Post (پوسٹ بنائیں)" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.ImageDetailActivity"
            android:configChanges="orientation|screenSize"
            android:label="Images(تصاویر)"
            android:theme="@style/AppCompat.Black.NoActionBar" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.LoginActivity"
            android:label="Sign In(داخلہ)"
            android:theme="@style/AppCompat.NoActionBar" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.CreateProfileActivity"
            android:configChanges="orientation|screenSize"
            android:label="Create profile(پروفائل بنائیں)" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.ProfileActivity"
            android:configChanges="orientation|screenSize"
            android:label="Profile (پروفائل)"
            android:theme="@style/AppCompat.NoActionBar" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.EditProfileActivity"
            android:configChanges="orientation|screenSize"
            android:label="Edit Profile (پروفائل میں ترمیم کریں)" />
        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.activities.EditPostActivity"
            android:configChanges="orientation|screenSize"
            android:label="Edit Post(پوسٹ میں ترمیم کریں)" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.activities.BaseActivity" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="1b9fd82ec4fd18a3d833df072e3a8d6429327cde" />

        <service android:name="com.kashdevelopment.development.kashmirstudentportal.services.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.kashdevelopment.development.kashmirstudentportal.services.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.kashdevelopment.development.kashmirstudentportal.Main2Activity"
            android:label=
        "Kashmir Student کشمیر طالب علم"
            android:theme="@style/AppTheme.NoActionBar"
            />

        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.ResultsActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.AdmissionsActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.EntranceActivity"/>
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.DateSheetActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.PreviousPapersActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.PGActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.GraduationActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.twelveActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.tenthActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.tipsActivity"/>
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.notespgmedbedActivity"/>
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.GradActivity" />
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.tenth12thnotesActivity"/>
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.gamesActivity"/>
        <activity android:name="com.kashdevelopment.development.kashmirstudentportal.FAQActivity">

        </activity>
    </application>

</manifest>