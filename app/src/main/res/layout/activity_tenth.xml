<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/tenthtwethnotes"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    tools:context="com.kashdevelopment.development.kashmirstudentportal.tenthActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="10th CLASS"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="36sp"
        tools:text="10th CLASS" />

    <Button
        android:id="@+id/button53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="28dp"
        android:onClick="eng10"
        android:text="English" />

    <Button
        android:id="@+id/button61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button53"
        android:layout_alignBottom="@+id/button53"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:onClick="mat10"
        android:text="Math" />

    <Button
        android:id="@+id/button62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button53"
        android:layout_marginTop="47dp"
        android:onClick="uru10"
        android:text="urdu" />

    <Button
        android:id="@+id/button63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button62"
        android:layout_alignEnd="@+id/button61"
        android:layout_alignRight="@+id/button61"
        android:onClick="bio10"
        android:text="biology" />

    <Button
        android:id="@+id/button64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button62"
        android:layout_marginTop="63dp"
        android:onClick="civ10"
        android:text="civics" />

    <Button
        android:id="@+id/button65"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button64"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="59dp"
        android:text="notes for 10th" />

    <Button
        android:id="@+id/button69"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button71"
        android:layout_marginBottom="16dp"
        android:layout_toEndOf="@+id/button63"
        android:layout_toRightOf="@+id/button63"
        android:onClick="math9"
        android:text="9th math" />

    <Button
        android:id="@+id/button70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button69"
        android:layout_toLeftOf="@+id/button69"
        android:layout_toStartOf="@+id/button69"
        android:onClick="che10"
        android:text="chemistry" />

    <Button
        android:id="@+id/button71"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button64"
        android:layout_toLeftOf="@+id/button70"
        android:layout_toStartOf="@+id/button70"
        android:text="geography"
        android:onClick="geo10"
        android:textSize="12sp" />

    <Button
        android:id="@+id/button72"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button69"
        android:layout_alignStart="@+id/button69"
        android:layout_alignTop="@+id/button63"
        android:onClick="phy10"
        android:text="physics" />

    <Button
        android:id="@+id/button73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button69"
        android:layout_alignRight="@+id/button69"
        android:layout_centerVertical="true"
        android:onClick="his10"
        android:text="history" />

    <Button
        android:id="@+id/button114"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_below="@+id/button69"
        android:text="promos" />


</RelativeLayout>
